{
  "name": "full-moon-night",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "pnpm --filter react-game-ui run build && vite build",
    "preview": "vite preview",
    "start": "node dist/server.js",
    "dev:multi": "concurrently \"npm run start\" \"vite --host\""
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/rollup-linux-x64-gnu": "^4.52.4",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "^9.2.1",
    "rollup": "4.20.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "express": "^5.1.0",
    "vite": "^7.1.7",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-static-copy": "^3.1.3"
  },
  "dependencies": {
    "react": "18.3.1", 
    "react-dom": "18.3.1", 
    "react-game-ui": "github:kei-0021/react-game-ui#issue-10",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-dom": "18.3.1"
    }
  }
}
